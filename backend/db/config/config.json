{
    "development": {
        "storage": "./db.sqlite",
        "dialect": "sqlite",
        "seederStorage": "sequelize",
        "logQueryParameters": true,
        "typeValidation": true
    },
    "production": {
        "use_env_variable": "DATABASE_URL",
        "dialect": "postgres",
        "seederStorage": "sequelize",
        "dialectOptions": {
            "ssl": {
                "require": true,
                "rejectUnauthorized": false
            }
        },
        "define": {
            "schema": "your_schema_name"
        }
    }
}
